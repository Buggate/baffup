        <meta property="fb:admins" content="Facebook numberic ID" />


    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': true %> 

    <%= stylesheet_link_tag    'partyform' %>
    
      
       <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

       <link href="https://fonts.googleapis.com/css?family=Oswald|Inconsolata:italic|Averia+Serif+Libre|Courgette|Handlee|Parisienne|Sacramento|Julius+Sans+One|Raleway" rel="stylesheet"> 

      <style>
      
      body {
             
            font-family: 'Handlee', cursive;
            font-family: 'Sacramento', cursive;
            font-family: 'Parisienne', cursive;
            font-family: 'Averia+Serif+Libre', cursive;
            font-family: 'Courgette', cursive;
            font-family: 'Inconsolata:italic', cursive;
            font-family: 'Oswald', cursive;
            font-family: 'Julius Sans One', sans-serif;
            font-family: 'Raleway', sans-serif;
          
                       
          }
    </style>
    
       <%= csrf_meta_tags %>
    
</head>

    <body>
		
		<!-- Top menu -->
	
        
        <!-- Description -->
		<div class="description-container">
	        <div class="container">
	        	<div class="row">
	                <div class="col-sm-12 description-title"><br /><br /><br /><br /><br /><br />
	                    <h2 style="font-family: 'Julius Sans One', sans-serif; font-size: 38px; color: red">Your Fun Starts!!! Loading Your Occassions</h2>
	                </div>
	            </div>
	            
	        </div>

       </div>
	

		<div>

		   <%= yield %>

		</div>

		


		
		<!-- Multi Step Form -->
		<div class="msf-container">
	        <div class="container">
	        	<div class="row">
	                <div class="col-sm-12 msf-title">

	                    <h3 style="font-family: 'Raleway', sans-serif; font-size: 22px; font-weight: italic; color: black">ENJOY YOUR TIME HERE</h3>
	                    <p style="font-family: 'Raleway', sans-serif; font-size: 22px; font-weight: italic; color: black">A trial will convince you, its features will make you go gaga</p>
                        <br />
                        <br />

                        <strong><%= link_to 'Back', profiles_path(:profile_id => current_user.profile.id), :type => "button", :class => "btn btn-default", :style => "font-family: 'Handlee', serif; font-size: 18px;" %></strong>
	                </div>
	            </div>
	        
	            				
	            
			</div>
		</div>

        <!-- Javascript -->
         <%= javascript_include_tag "application", 'data-turbolinks-track': true %>

 
