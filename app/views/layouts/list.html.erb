</head>

  <body class="is-loading-0 is-loading-1 is-loading-2">

  <!-- Google Tag Manager (noscript) -->

<!-- End Google Tag Manager (noscript) -->s

    <!-- Main -->
      <div id="main">

        <!-- Header -->
        
          <header id="header">

         


          <%= link_to "mYCatalog", profiles_path, :type => "button", :class => "btn btn-danger", :style => "font-family: 'Montserrat', cursive; color: #00193a; font-size: 14px; font-weight: bold;" %>&nbsp;&nbsp;&nbsp;&nbsp;



          <%= link_to @party.title, visitor_path(@visitor), :type => "button", :class => "btn btn-info", :style => "font-family: 'Montserrat', cursive; color: #915b05; font-size: 14px; font-weight: bold;" %><br />

          <br />


          <h2 style="font-family: 'Montserrat', sans-serif; font-size: 18px; color: #00193a;">Start preparing for your great events, party, ceremonies etc <br />
              from here. Its easy and free</h2>
           
           
          </header>



        <!-- Thumbnail -->

          <section id="thumbnails">

          <% @party.catalogues.each do |catalogue| %> 

            <article>

              <a class="thumbnail" href="<%= catalogue.file_name.url %>" data-position="left center" >
         
              <%=  image_tag catalogue.file_name.url %>
                
              
              </a>

            
            </article>



             <% end %>


            
          </section>

          

  </body>

  <%= javascript_include_tag "application", 'data-turbolinks-track': :reload %>

