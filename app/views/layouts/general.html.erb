        <li><%= link_to "" %></li>


      <li><%= link_to 'Date', edit_date_parties_path(@party), :type => "button", :class => "btn btn-default", :style => "font-family: 'Julius Sans One', sans-serif; font-size: 14px; color: black; font-weight: bold;"  %></li>
                  

